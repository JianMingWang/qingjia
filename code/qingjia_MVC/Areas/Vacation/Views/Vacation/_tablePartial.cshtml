@model IEnumerable<qingjia_MVC.Areas.Vacation.Controllers.LL_Table>
@{
    
    Layout = null;
}

<table class="table table-hover table-bordered" style="text-align:center; margin-top:10px;">
    <caption style="font-size:25px;">@ViewBag.tableTitle</caption>
    <tr>
        <th style="text-align:center;">
            学号
        </th>
        <th style="text-align:center;">
            班级
        </th>
        <th style="text-align:center;">
            姓名
        </th>
        <th style="text-align:center;">
            节假日去向
        </th>
        <th style="text-align:center;">
            离校时间
        </th>
        <th style="text-align:center;">
            返校时间
        </th>
        <th style="text-align:center;">
            离校方式
        </th>
        <th style="text-align:center;">
            返校方式
        </th>
        <th style="text-align:center;">
            离校去向地址
        </th>
        <th style="text-align:center;">
            联系人
        </th>
        <th style="text-align:center;">
            联系方式
        </th>
        <th style="text-align:center;">
            签名确认
        </th>
    </tr>
    @foreach (var tableRow in Model)
    {
        <tr>
            <td>
                @tableRow.ST_Num
            </td>
            <td>
                @tableRow.ST_Class
            </td>
            <td>
                @tableRow.ST_Name
            </td>
            <td>
                @tableRow.ST_Go
            </td>
            <td>
                @tableRow.TimeLeave
            </td>
            <td>
                @tableRow.TimeBack
            </td>
            <td>
                @tableRow.LeaveWay
            </td>
            <td>
                @tableRow.BackWay
            </td>
            <td>
                @tableRow.Address
            </td>
            <td>
                @tableRow.Contact
            </td>
            <td>
                @tableRow.Tel
            </td>
            <td>

            </td>
        </tr>
    }
</table>



